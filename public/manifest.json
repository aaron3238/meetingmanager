{
  "name": "Meeting Manager",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Manage your zoom links",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage"
    ],
  "icons": {
      "512": "logo512.png"
  },
  "browser_action": {
      "default_icon": "logo512.png",
      "default_popup": "popup.html",
      "default_title": "MeetingManager"
  }
}